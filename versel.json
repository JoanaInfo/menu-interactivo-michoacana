{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "github": {
    "silent": true
  },
  "files": ["app.py", "templates/**", "static/**", "data/**", "requirements.txt"] 
}
